{
  "name": "raw-blog",
  "version": "1.0.0",
  "private": true,
  "description": "An intuitive blog for those who are tired of the glamor of modern web apps",
  "keywords": [
    "blog"
  ],
  "author": {
    "name": "zuhno",
    "email": "zuhno11@gmail.com"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a",
  "scripts": {
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "@client": "cd apps/client && corepack pnpm",
    "@server": "cd apps/server && corepack pnpm",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --cache --max-warnings=0 --debug",
      "prettier --write"
    ],
    "*.{json,md,css,scss,yml,yaml}": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "es-toolkit": "^1.39.9"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
